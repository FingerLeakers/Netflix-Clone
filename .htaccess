<IfModule mod_rewrite.c>
 RewriteEngine On
RewriteRule ^register /index.php?register [L]
RewriteRule ^login /index.php?login[L]
RewriteRule ^forgot /index.php?forgot[L]
RewriteRule ^dashboard /index.php?dashboard[L]
RewriteRule ^browse /index.php?browse[L]
RewriteRule ^createprofile /index.php?createprofile[L]
RewriteRule ^editprofile /index.php?editprofile[L]
RewriteRule ^logando /app/backend/loginFunction.php
RewriteRule ^registro /app/backend/registerFunction.php
RewriteRule ^newprofile /app/backend/newProfile.php
RewriteRule ^swift/(.*)$ /app/backend/swift.php?id=$1 [L]
RewriteRule ^editandoprofile/(.*)$ index.php?id=$1&editandoprofile=1 [L]
RewriteRule ^deleteprofile/(.*)$ /app/backend/deleteprofile.php?id=$1 [L]
RewriteRule ^series /index.php?series[L]
RewriteRule ^movies /index.php?filmes[L]
RewriteRule ^news /index.php?news[L]
RewriteRule ^myplaylist /index.php?playlist[L]
RewriteRule ^genero/(.*)$ index.php?id=$1&genero [L]
RewriteRule ^generoblob/(.*)$ index.php?id=$1&generoblob [L]
RewriteRule ^title/(.*)$ index.php?id=$1&title=1 [L]
RewriteRule ^watch/(.*)$ index.php?id=$1&watch [L]
</IfModule>